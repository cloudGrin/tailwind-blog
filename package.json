{
  "name": "starter-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "start": "next dev",
    "dev": "npm run codegen && cross-env INIT_CWD=$PWD next dev",
    "build": "npm run codegen && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node -r esbuild-register ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.7.0",
    "@as-integrations/next": "^1.3.0",
    "@next/bundle-analyzer": "13.3.1",
    "@prisma/client": "^4.13.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "contentlayer": "0.3.1",
    "esbuild": "0.17.18",
    "github-slugger": "^2.0.0",
    "graphql": "^16.6.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "mdx-bundler": "^9.2.1",
    "next": "13.3.1",
    "next-contentlayer": "0.3.1",
    "next-themes": "^0.2.1",
    "pliny": "0.0.10",
    "postcss": "^8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.1",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.3.2",
    "unist-util-visit": "^4.1.2",
    "urql": "^4.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@svgr/webpack": "^7.0.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "3.4.2",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "globby": "11.0.3",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prisma": "^4.13.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}