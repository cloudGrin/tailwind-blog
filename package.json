{
  "name": "starter-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "start": "next dev",
    "dev": "npm run codegen && cross-env INIT_CWD=$PWD next dev",
    "build": "contentlayer build && npm run codegen && cross-env INIT_CWD=$PWD next build",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir src --dir scripts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@as-integrations/next": "^2.0.1",
    "@next/bundle-analyzer": "13.4.7",
    "@prisma/client": "^4.16.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "contentlayer": "0.3.3",
    "esbuild": "0.18.10",
    "github-slugger": "^2.0.0",
    "graphql": "^16.7.1",
    "image-size": "1.0.2",
    "next": "13.4.7",
    "next-contentlayer": "0.3.3",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "server-only": "^0.0.1",
    "tailwindcss": "^3.3.2",
    "unist-util-visit": "^4.1.2",
    "urql": "^4.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@svgr/webpack": "^8.0.1",
    "@types/mdast": "^3.0.11",
    "@types/react": "^18.2.14",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "eslint": "^8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.2.3",
    "mdast": "^3.0.0",
    "mdast-util-to-string": "^3.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.1",
    "typescript": "^5.1.6",
    "vfile": "^5.3.7"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}